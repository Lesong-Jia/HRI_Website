uted: {\n    ...mapGetters([\"gameStep\", \"allQuestionForm\"]),\n  },\n  methods: {\n    handleSubmit() {\n      const that = this;\n      this.$refs[\"form\"].validate(async (valid) => {\n        if (!valid) {\n          return;\n        }\n        //保存最后一次问卷\n        that.$store.commit(\"SET_ALL_QUESTION_FORM\", {\n          ...that.allQuestionForm,\n          last: { ...that.form },\n        });\n        const res = await api.submitForm(this.allQuestionForm);\n        if (res.code === 200) {\n          this.$message.success(\"Submitted successfully\");\n          this.$emit(\"changeLoading\", false);\n          that.$router.push(\"/last\");\n        } else {\n          this.$emit(\"changeLoading\", true);\n          this.$message.error(res.message);\n        }\n      });\n    },\n    checkRadio(item, key) {\n      this.$set(this.form.table, key, item);\n    },\n  },\n};\n</script>\n<style lang=\"less\" scoped>\n::v-deep .ant-input {\n  border: none !important;\n  width: 100%;\n}\n::v-deep .ant-input:focus {\n  box-shadow: none;\n}\n</style>"],"mappings":";AAuDA,SAAAA,UAAA;AACA,OAAAC,GAAA;AACA,SAAAC,SAAA;AACA,SAAAC,SAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA,EAAAH,SAAA;MACAD,SAAA;MACAK,MAAA;MACAC,IAAA;QACAC,KAAA;MACA;MACAC,MAAA;MACAC,UAAA;QACAC,OAAA;QACAC,MAAA;QACAC,UAAA;MACA;MACAC,QAAA;QACAC,EAAA;UAAAC,IAAA;QAAA;QACAC,EAAA;UAAAD,IAAA;QAAA;MACA;MACAE,UAAA;QACAH,EAAA;UAAAC,IAAA;QAAA;QACAC,EAAA;UAAAD,IAAA;QAAA;MACA;IACA;EACA;EACAG,QAAA;EACAC,QAAA;IACA,GAAArB,UAAA;EACA;EACAsB,OAAA;IACAC,aAAA;MACA,MAAAC,IAAA;MACA,KAAAC,KAAA,SAAAC,QAAA,OAAAC,KAAA;QACA,KAAAA,KAAA;UACA;QACA;QACA;QACAH,IAAA,CAAAI,MAAA,CAAAC,MAAA;UACA,GAAAL,IAAA,CAAAM,eAAA;UACAC,IAAA;YAAA,GAAAP,IAAA,CAAAhB;UAAA;QACA;QACA,MAAAwB,GAAA,SAAA/B,GAAA,CAAAgC,UAAA,MAAAH,eAAA;QACA,IAAAE,GAAA,CAAAE,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAC,KAAA;UACAb,IAAA,CAAAc,OAAA,CAAAC,IAAA;QACA;UACA,KAAAF,KAAA;UACA,KAAAF,QAAA,CAAAK,KAAA,CAAAR,GAAA,CAAAS,OAAA;QACA;MACA;IACA;IACAC,WAAAC,IAAA,EAAAC,GAAA;MACA,KAAAC,IAAA,MAAArC